<!-- This is the base xml for all physics simulations. Set global configs here. -->
<mujoco model="base">
    <compiler angle="radian" meshdir="meshes/"/>
    <option impratio="20" cone="elliptic" integrator="RK4"/>
    <size nconmax="5000" njmax="5000"/>
    <visual>
        <map znear="0.001"/>
    </visual>

    <asset>
        <texture name="metal" file="textures/metal.png" type="2d"/>
        <material name="metal" texture="metal"/>
        <texture name="dark-wood" file="textures/dark-wood.png" type="2d"/>
        <material name="dark-wood" texture="dark-wood"/>
        <texture name="light-wood" file="textures/light-wood.png" type="2d"/>
        <material name="light-wood" texture="light-wood"/>
    </asset>

    <actuator>
    </actuator>

    <worldbody>
    </worldbody>

    <equality>
    </equality>
</mujoco>
