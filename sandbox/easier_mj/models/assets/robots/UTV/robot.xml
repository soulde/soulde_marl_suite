<!-- Modified for furniture environment -->

<mujoco model="utv">
    <actuator>
        <velocity ctrllimited="true" ctrlrange="-7 7" joint="joint_front_left_wheel" kv="40"
                  name="vel_front_left_wheel"/>
                <velocity ctrllimited="true" ctrlrange="-7 7" joint="joint_front_right_wheel" kv="40"
                  name="vel_front_right_wheel"/>
        <velocity ctrllimited="true" ctrlrange="-7 7" joint="joint_rear_left_wheel" kv="40" name="vel_rear_left_wheel"/>

        <velocity ctrllimited="true" ctrlrange="-7 7" joint="joint_rear_right_wheel" kv="40"
                  name="vel_rear_right_wheel"/>
    </actuator>
    <asset>
        <mesh name="base_link" file="meshes/base_link.stl"/>
        <mesh name="wheel_link" file="meshes/wheel.stl"/>
